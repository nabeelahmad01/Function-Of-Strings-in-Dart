void main(){
  
 String name="Nabeel Ahmad";
  print("$name");
 print(name.isEmpty);
  print(name.isNotEmpty);
  print(name.length);
  print(name.toLowerCase());
  print(name.toUpperCase());
  print(name.contains('b',3));
  print(name.replaceAll('Ahmad','Khan'));
  print(name.split(" "));
  print(name[8]);
  print(name.indexOf('b'));
  print(name.endsWith('d'));
  print(name.startsWith('a'));
  
 
  
  
}
